{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "apollo-boost": "^0.3.1",
    "apollo-link-ws": "^1.0.17",
    "babel": "^6.23.0",
    "babel-jest": "^24.7.1",
    "babel-plugin-graphql-tag": "^2.0.0",
    "graphql": "^14.1.1",
    "jest": "^24.7.1",
    "react": "^16.8.4",
    "react-apollo": "^2.5.2",
    "react-dom": "^16.8.4",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "subscriptions-transport-ws": "^0.9.16",
